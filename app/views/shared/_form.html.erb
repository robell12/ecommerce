<%= form_for :current_user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br />
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br />
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br />
  <%= f.label :input %>
  <%= f.text_area :input %>
  <br />
  <%= f.submit %>
<% end %>
